working stuff
	if(maxVec < adjX*adjX+adjY*adjY) maxVec = adjX*adjX+adjY*adjY;
	adjZ =  sqrt( maxVec-(adjX*adjX)-(adjY*adjY) );

	float finalX, finalY;
	finalX = adjX*cos(-eigenPitch)							+ adjZ*sin(-eigenPitch);
	finalY = adjX*sin(eigenRoll)*sin(-eigenPitch)	+ adjY*cos(eigenRoll)	- adjZ*sin(eigenRoll)*cos(-eigenPitch);

